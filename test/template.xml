    <Creatives>
      <Creative>
        <Linear>
          <TrackingEvents>
            <Tracking event="create"><![CDATA[${scheme}://${host}/v1/${consumer}/${platform}/tracking?e=create&eid=${eventId}&st=${enumSourceType}&sn=${base64SourceName}&cid=${clientId}&placetype=${placeType}&adId=${adId}]]></Tracking>
