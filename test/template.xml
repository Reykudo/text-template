            <Tracking event="trueStart"><![CDATA[${scheme}://${host}/v1/${consumer}/${platform}/tracking?e=trueStart&eid=${eventId}&st=${enumSourceType}&sn=${base64SourceName}&cid=${clientId}&placetype=${placeType}&adId=${adId}]]></Tracking>
