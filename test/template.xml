          <VideoClicks>
            <ClickTracking>
              <![CDATA[${scheme}://${host}/v1/${consumer}/${platform}/tracking?e=click&eid=${eventId}&st=${enumSourceType}&sn=${base64SourceName}&cid=${clientId}&placetype=${placeType}&adId=${adId}]]>
            </ClickTracking>
          </VideoClicks>
        </Linear>
      </Creative>
    </Creatives>
  </Wrapper>
</Ad>